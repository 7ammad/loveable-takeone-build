{
  "name": "enter-tech",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "contracts:build": "redocly bundle ./packages/core-contracts/openapi.yaml -o ./packages/core-contracts/dist/openapi.json",
    "contracts:test": "cross-env NODE_ENV=test dredd --config dredd.yml",
    "migrate:dev": "prisma migrate dev",
    "seed": "dotenv -e .env -- tsx scripts/seed.ts",
    "search:bootstrap": "dotenv -e .env -- tsx scripts/search/bootstrap-algolia.ts",
    "worker": "dotenv -e .env -- tsx worker/src/index.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.894.0",
    "@aws-sdk/s3-presigned-post": "^3.894.0",
    "@aws-sdk/s3-request-presigner": "^3.893.0",
    "@opentelemetry/exporter-otlp-http": "^0.26.0",
    "@prisma/client": "^5.22.0",
    "@redocly/cli": "^2.1.5",
    "@sentry/nextjs": "^10.14.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.4",
    "algoliasearch": "^5.37.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.7",
    "ioredis": "5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "next": "15.5.3",
    "openapi-zod-client": "^1.18.3",
    "rate-limiter-flexible": "^7.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "dredd": "^14.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "hooks": "^0.3.2",
    "jsdom": "^27.0.0",
    "prisma": "^5.17.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "schema": "packages/core-db/prisma/schema.prisma"
  }
}
